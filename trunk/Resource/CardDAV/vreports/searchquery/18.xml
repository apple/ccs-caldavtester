<?xml version="1.0" encoding="utf-8" ?>
   <C:addressbook-query xmlns:D="DAV:"
                     xmlns:C="urn:ietf:params:xml:ns:carddav">
     <D:prop>
       <D:getetag/>
       <C:address-data>
         <C:prop name="VERSION"/>
         <C:prop name="UID"/>
         <C:prop name="NICKNAME"/>
         <C:prop name="EMAIL"/>
         <C:prop name="FN"/>
       </C:address-data>
     </D:prop>
     <C:filter test="allof">
       <C:prop-filter name="KIND" test="anyof">
         <C:text-match collation="i;unicode-casemap"
                       match-type="equals"
         >individual</C:text-match>
         <C:is-not-defined/>
       </C:prop-filter>
       <C:prop-filter name="FN" test="anyof">
         <C:text-match collation="i;unicode-casemap"
                       match-type="contains"
         >03</C:text-match>
       </C:prop-filter>
      </C:filter>
   </C:addressbook-query>