<?xml version="1.0" encoding="utf-8" ?>
   <C:addressbook-query xmlns:D="DAV:"
                     xmlns:C="urn:ietf:params:xml:ns:carddav">
     <D:prop>
       <D:getetag/>
       <C:address-data>
         <C:prop name="EMAIL"/>
       </C:address-data>
     </D:prop>
     <C:filter>
       <C:prop-filter name="VERSION">
         <C:text-match collation="i;unicode-casemap"
                       match-type="ends-with"
         >.0</C:text-match>
       </C:prop-filter>
     </C:filter>
     <C:limit>
       <C:nresults>10</C:nresults>
     </C:limit>
   </C:addressbook-query>
