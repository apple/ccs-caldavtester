			<method>POST</method>
			<ruri>$calendarhome3:/</ruri>
			<data substitutions='yes'>
				<content-type>application/xml; charset=utf-8</content-type>
				<filepath>Resource/Common/POST/sharingreply3.xml</filepath>
			</data>
			<grabelement>
				<name>{DAV:}href</name>
