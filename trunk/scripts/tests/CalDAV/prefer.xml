					<content-type>text/xml; charset=utf-8</content-type>
					<filepath>Resource/CalDAV/prefer/minimal/1.xml</filepath>
				</data>
				<verify>
					<callback>propfindItems</callback>
					<arg>
						<name>okprops</name>
