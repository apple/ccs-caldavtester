					<arg>
						<name>error</name>
						<value>{DAV:}recognized-principal</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='9' ignore='no'>
			<require-feature>
				<feature>per-object-ACLs</feature>
			</require-feature>
			<description>Valid command</description>
			<request print-response='no'>
				<method>ACL</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<data>
					<content-type>text/xml; charset=utf-8</content-type>
					<filepath>Resource/CalDAV/acls/12.xml</filepath>
				</data>
				<verify>
					<callback>prepostcondition</callback>
					<arg>
						<name>error</name>
						<value>{DAV:}recognized-principal</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='10' ignore='no'>
			<require-feature>
				<feature>per-object-ACLs</feature>
			</require-feature>
			<description>Valid command</description>
			<request print-response='no'>
				<method>ACL</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<data>
					<content-type>text/xml; charset=utf-8</content-type>
					<filepath>Resource/CalDAV/acls/22.xml</filepath>
				</data>
				<verify>
					<callback>prepostcondition</callback>
					<arg>
						<name>error</name>
						<value>{DAV:}recognized-principal</value>
					</arg>
				</verify>
			</request>
		</test>
	</test-suite>
	
	<test-suite name='COPY/MOVE interaction with ACL' ignore='no'>
		<test name='1' ignore='no'>
			<description>COPY to non-writeable shared calendar</description>
			<request print-response='no'>
				<method>COPY</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<header>
					<name>Destination</name>
					<value>$host:$calendarhome2:/visibletouser01/copy1.ics</value>
				</header>
				<verify>
					<callback>statusCode</callback>
					<arg>
						<name>status</name>
						<value>403</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='2' ignore='no'>
			<description>COPY to non-visible shared calendar</description>
			<request print-response='no'>
				<method>COPY</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<header>
					<name>Destination</name>
					<value>$host:$calendarhome2:/notvisibletouser01/copy1.ics</value>
				</header>
				<verify>
					<callback>statusCode</callback>
					<arg>
						<name>status</name>
						<value>403</value>
					</arg>
				</verify>
			</request>
		</test>
		<test name='3' ignore='no'>
			<description>COPY to non-visible shared calendar with overwrite</description>
			<request print-response='no'>
				<method>COPY</method>
				<ruri>$calendarpath1:/1.ics</ruri>
				<header>
					<name>Destination</name>
					<value>$host:$calendarhome2:/notvisibletouser01/hidden1.ics</value>
