<caldavtest>
	<description>Test implicit scheduling for one attendee, not recurring</description>

	<require-feature>
		<feature>caldav</feature>
		<feature>implicit-scheduling</feature>
	</require-feature>

	<start>
		<request user="$userid1:" pswd="$pswd1:">
			<method>GET</method>
			<ruri>$calendarpath1:/</ruri>
		</request>
		<request user="$userid2:" pswd="$pswd2:">
			<method>GET</method>
			<ruri>$calendarpath2:/</ruri>
		</request>
		<request user="$userid3:" pswd="$pswd3:">
			<method>GET</method>
			<ruri>$calendarpath3:/</ruri>
		</request>
	</start>
	
	<test-suite name='Empty Collection Delete' ignore='no'>
		<require-feature>
			<feature>regular-collection</feature>
		</require-feature>

		<test name='1'>
			<description>Organizer creates collection</description>
			<request print-response='no'>
				<method>MKCOL</method>
				<ruri>$calendarhome1:/collection/</ruri>
				<verify>
					<callback>statusCode</callback>
				</verify>
			</request>
		</test>
		<test name='2'>
			<description>Organizer deletes collection</description>
			<request print-response='no'>
				<method>DELETE</method>
				<ruri>$calendarhome1:/collection/</ruri>
				<header>
					<name>Depth</name>
					<value>infinity</value>
				</header>
				<verify>
					<callback>statusCode</callback>
				</verify>
			</request>
		</test>
		<test name='3'>
			<description>Organizer collection is gone</description>
			<request print-response='no'>
				<method>GET</method>
				<ruri>$calendarhome1:/collection/</ruri>
				<verify>
					<callback>statusCode</callback>
					<arg>
						<name>status</name>
						<value>404</value>
					</arg>
				</verify>
			</request>
		</test>
	</test-suite>
	
