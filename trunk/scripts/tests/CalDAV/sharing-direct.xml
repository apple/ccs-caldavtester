		</test>
		<test name='12'>
			<description>Sharer sees changed event</description>
			<request print-response='no'>
				<method>GET</method>
				<ruri>$calendarhome1:/shared/2.ics</ruri>
				<verify>
					<callback>calendarDataMatch</callback>
