					<content-type>text/xml; charset=utf-8</content-type>
					<filepath>Resource/CalDAV/sharing/notification-sync/2.xml</filepath>
				</data>
				<verify>
					<callback>multistatusItems</callback>
					<arg>
						<name>totalcount</name>
